		Note N4= new Note("math",12);
		Note N5= new Note("math",12);
		Note N6= new Note("math",15);
		et2.addNote(N2);
		
		Note N7 = new Note("math",12);
		Note N8 = new Note("math",12);
		Note N9= new Note("math",15);
		et3.addNote(N2);
		
		System.out.print(et1.getNote());
		
		
	}

