package ex2;

public class TacheElementaire implements Tache {
	
	private String nom;
	private double cout;
	

	public TacheElementaire(String nom, double cout) {
		super();
		this.nom = nom;
		this.cout = cout;
	}
	
	@Override
	public String getNom() {
		return nom;
	}

	@Override
	public double getCout() {
		return cout;
	}

	

}
