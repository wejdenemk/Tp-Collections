		this.cout = cout;
	}
	

	

	public String getNom() {
		return nom;
	}




	public void setNom(String nom) {
		this.nom = nom;
	}





	@Override
	public double getNom() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public double getCout() {
		// TODO Auto-generated method stub
		return 0;
	}

	

}
