package ex1;

public class Main {
	
	public static void main(String[] args) {
		
		Etudiant et1 = new Etudiant("mat1","Nadou");
		Etudiant et2 = new Etudiant("mat2","Ahmed");
		Etudiant et3 = new Etudiant("mat3","Mouhamed");
		Etudiant et4 = new Etudiant("mat4","Mouhib");
		
		Note N1 = new Note ("math",12);
		et1.addNote(N1);
		
		Note N2= new Note ("math",15);
		et1.addNote(N2);
		
		System.out.print(et1.getNote());
		
		
	}

}

